<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.logistics.mapper.AccountMapper">
    <select id="findByUsernameAndPassword" resultType="com.company.logistics.model.Account">
        SELECT * FROM accountTable WHERE Username = #{username} AND Password = #{password}
    </select>
</mapper>

